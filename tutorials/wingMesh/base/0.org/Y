FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Y;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 0 0 0 0 0 0];

internalField       uniform 0.00633;

boundaryField
{

    inlet
    {
	type		fixedValue;
	value		uniform 0.00633;
    }

    outlet
    {
	type		zeroGradient; // inletOutlet; // 
	inletValue	$internalField;
	value		$internalField;
    }

    wing
    {
/*
	type		fixedValue; // zeroGradient; // 
	value		uniform 0.001;
*/
	type		groovyBC; // fixedValue; // 

	variables
	(
	    "rh=0.7;"
	);

	valueExpression
	    "YSat*rh";

    }

    topAndBottom
    {
	type		slip;
    }

    front
    {
	type		empty;
    }

    back
    {
	type		empty;
    }

}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
