FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [0 0 0 0 0 0 0];

internalField       uniform 1;

boundaryField
{

    inlet
    {
	type		fixedValue;
	value		$internalField;
    }

    outlet
    {
	type		zeroGradient; // fixedValue; // 
	value		$internalField;
    }

    wing
    {
	type		calculated; // zeroGradient; // 
	value		$internalField;
    }

    topAndBottom
    {
	type		slip;
    }

    front
    {
	type		empty;
    }

    back
    {
	type		empty;
    }

}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
