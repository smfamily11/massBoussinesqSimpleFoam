FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    object      setFieldsDict;
}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

defaultFieldValues
(
    //volScalarFieldValue T 300
    volScalarFieldValue ice1:alpha1 1
);

regions
(
    // Set patch values (using ==)
    boxToCell
    {
        //box (4 -1 -1) (6 1e-4 1);
	box (4 0 0) (6 12 0.1);

        fieldValues
        (
            //volScalarFieldValue T 400
	    volScalarFieldValue ice1:alpha1 0
        );
    }
);

