FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -2 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    in
    {
	type            zeroGradient; // fixedFluxPressure; // fixedValue; // 
        //gradient        uniform 0;
	value           uniform 0;
    }

    out
    {
	type            fixedValue;
	value           uniform 0;
    }

    bottom
    {
	type            zeroGradient; // fixedFluxPressure; // fixedValue; // 
	value           uniform 0;
    }

    top
    {
	type		empty;
    }

    frontAndBack
    {
	type		empty;
    }
}

