#!/bin/sh
cd ${0%/*} || exit 1    # run from this directory

# Source tutorial run functions
. $WM_PROJECT_DIR/bin/tools/RunFunctions

application=`getApplication`

cp system/fvScheme/fvSchemes_steady system/fvSchemes
cp 0.org/T_Differ/T_260 0.org/T

# Cycle #1 - T260_AOA 00
cp system/controlDict_AOA_steady/controlDict_aoa00 system/controlDict

runApplication extrudeMesh
runApplication createPatch -overwrite

cp -r 0.org 0

cp 0/U15_AOA/U_aoa00 0/U

runApplication massBoussinesqSimpleFoam

foamToVTK

mv VTK VTK_T260_AOA00

mv postProcessing aero_T260_aoa00

