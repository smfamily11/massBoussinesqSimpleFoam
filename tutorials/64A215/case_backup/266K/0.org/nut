FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1.6e-4; // 0; //

boundaryField
{

    inlet
    {
	type		fixedValue;
	value		$internalField;
    }

    outlet
    {
	type		zeroGradient;
	value		$internalField;
    }

    wing
    {
	type		nutkWallFunction;
	value		uniform 1.6e-4;
    }

    topAndBottom
    {
	type		slip; // nutkWallFunction;
	value		uniform 1.6e-4;
    }

    front
    {
	type		empty;
    }

    back
    {
	type		empty;
    }

}

// ************************************************************************* //
