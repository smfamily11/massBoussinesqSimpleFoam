FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      Sc;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions          [1 0 -3 -1 0 0 0];

internalField       uniform 0;

boundaryField
{

    inlet
    {
	type		calculated; // inletOutlet; // zeroGradient; // 
	value		$internalField;
    }

    outlet
    {
	type		calculated; // inletOutlet; // zeroGradient; // 
	inletValue	$internalField;
	value		$internalField;
    }

    wing
    {
	type		calculated; // fixedValue; // zeroGradient; // 
	value		$internalField; // uniform 0;
    }

    topAndBottom
    {
	type		slip;
    }

    front
    {
	type		empty;
    }

    back
    {
	type		empty;
    }

}

// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //
