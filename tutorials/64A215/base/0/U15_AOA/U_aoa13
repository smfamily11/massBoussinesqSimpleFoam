FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (14.616 3.374 0);

boundaryField
{

    inlet
    {
	type		fixedValue;
	value		$internalField;
    }

    outlet
    {
	type            inletOutlet; // zeroGradient; // 
	inletValue      uniform (0 0 0);
	value           $internalField;
    }

    wing
    {
	type		fixedValue;
	value		uniform (0 0 0);
    }

    topAndBottom
    {
	type		zeroGradient; // slip;
    }

    front
    {
	type		empty;
    }

    back
    {
	type		empty;
    }

}

// ************************************************************************* //
