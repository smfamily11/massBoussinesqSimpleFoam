FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      alphat;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1e-3;

boundaryField
{

    inlet
    {
        type            calculated;
        value           $internalField;
    }

    outlet
    {
        type            calculated;
        value           $internalField;
    }

    wing
    {
        type            alphatJayatillekeWallFunction; // calculated; // 
        Prt             0.85;
        value           $internalField;
    }

    topAndBottom
    {
        type            fixedValue; // calculated; // 
        Prt             0.85;
        value           $internalField;
    }

    front
    {
	type		empty;
    }

    back
    {
	type		empty;
    }

}


// ************************************************************************* //
