FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      epsilon;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -3 0 0 0 0];

internalField   uniform 200;

boundaryField
{

    inlet
    {
        type            turbulentMixingLengthDissipationRateInlet;
        mixingLength    0.005;
        value           uniform 200;
    }

    outlet
    {
        type            inletOutlet;
        inletValue      uniform 200;
        value           uniform 200;
    }

    wing
    {
        type            epsilonWallFunction;
        value           uniform 200;
    }

    topAndBottom
    {
	type		slip;
    }

    front
    {
	type		empty;
    }

    back
    {
	type		empty;
    }

}


// ************************************************************************* //
