FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "600";
    object      alpha1;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    topAndBottom
    {
        type            slip;
    }
    inlet
    {
        type            fixedValue;
        value           uniform 1;
    }
    outlet
    {
        type            zeroGradient;
    }
    wing
    {
        type            calculated;
        value           uniform 0.859586;
    }
    front
    {
        type            empty;
    }
    back
    {
        type            empty;
    }
}


